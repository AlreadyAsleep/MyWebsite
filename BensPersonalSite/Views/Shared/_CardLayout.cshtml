<div class="card border-dark">
    <div class="card-header bg-dark text-white">
        <h4 class="font-weight-light">@ViewBag.Title</h4>
    </div>
    <div class="card-body text-white">
        @RenderBody()
    </div>
    @if (ViewBag.Title != "Contact")
    {
        <div class="card-footer bg-primary">
            @Html.ActionLink("See More", ViewBag.Title as string, "Home", new { @class = "text-white" })
        </div>
    }


</div>
